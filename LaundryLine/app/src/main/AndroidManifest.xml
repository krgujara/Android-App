<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.retrofitpostexample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.LaundryApplication"
        android:allowBackup="true"
        android:background="@drawable/rounded_buuton"
        android:icon="@drawable/spinner1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".LaunchingPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AfterLogin" />
        <activity android:name=".RateCardList" />
        <activity
            android:name=".NavigationDrawer"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".NewOrderActivity" />
        <activity android:name=".PlaceOrder" />
        <activity android:name=".ConfirmOrder" />
        <activity android:name=".AcceptAddress" />
        <activity android:name=".OrderSummary" />
        <activity android:name=".ScrollingProfilleActivity" />
        <activity android:name=".MonthlyCombo" />
        <activity android:name=".aboutus" />
        <activity android:name=".contactus"></activity>
    </application>

</manifest>